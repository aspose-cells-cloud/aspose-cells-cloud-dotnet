<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net4.5.2;netstandard2.0</TargetFrameworks>
    <AssemblyName>Aspose.Cells.Cloud.SDK</AssemblyName>
    <PackageId>Aspose.Cells.Cloud.SDK</PackageId>
    <OutputType>Library</OutputType>
    <Authors>Aspose.Cells Cloud Team</Authors>
    <Company>Aspose</Company>
    <AssemblyTitle>Aspose.Cells Cloud SDK for .NET</AssemblyTitle>
    <Description>This SDK allows you to work with Aspose.Cells Cloud REST APIs in your .NET applications quickly and easily, with zero initial cost.</Description>
    <Copyright>MIT</Copyright>
    <StyleCopTreatErrorsAsWarnings>false</StyleCopTreatErrorsAsWarnings>    
    <AssemblyVersion>22.11</AssemblyVersion>
    <FileVersion>22.11</FileVersion>
    <Version>22.11</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="RestSharp" Version="106.2.2" />
  </ItemGroup>

</Project>
